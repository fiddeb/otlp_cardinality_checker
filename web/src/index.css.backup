/* Keep only non-conflicting legacy styles for non-migrated components */
  margin-bottom: 20px;
}

.stat-card {
  background: var(--bg-secondary);
  padding: 20px;
  border-radius: 8px;
  box-shadow: 0 1px 3px var(--shadow);
}

.stat-value {
  font-size: 32px;
  font-weight: 700;
  margin-bottom: 4px;
}

.stat-label {
  color: var(--text-secondary);
  font-size: 14px;
}

.memory-stats {
  margin-bottom: 20px;
}

.memory-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(180px, 1fr));
  gap: 16px;
  margin-top: 16px;
}

.memory-item {
  padding: 16px;
  background: var(--bg-tertiary);
  border-radius: 6px;
  border: 1px solid var(--border-color);
}

.memory-label {
  font-size: 12px;
  color: var(--text-secondary);
  text-transform: uppercase;
  letter-spacing: 0.5px;
  margin-bottom: 8px;
  display: flex;
  flex-direction: column;
  gap: 4px;
}

.memory-info {
  font-size: 10px;
  color: var(--text-secondary);
  opacity: 0.7;
  text-transform: none;
  letter-spacing: 0;
  font-weight: normal;
}

.memory-value {
  font-size: 24px;
  font-weight: 600;
  color: var(--text-primary);
}

table {
  width: 100%;
  border-collapse: collapse;
}

th {
  text-align: left;
  padding: 12px;
  background: var(--bg-tertiary);
  font-weight: 600;
  font-size: 13px;
  color: var(--text-secondary);
  border-bottom: 2px solid var(--border-color);
}

td {
  padding: 12px;
  border-bottom: 1px solid var(--border-color);
  font-size: 14px;
}

tr:hover {
  background: var(--bg-tertiary);
}

.badge {
  display: inline-block;
  padding: 4px 8px;
  border-radius: 4px;
  font-size: 12px;
  font-weight: 600;
}

.badge.high {
  background: #fee;
  color: #c00;
}

.badge.medium {
  background: #ffeaa7;
  color: #d63031;
}

.badge.low {
  background: #dfe6e9;
  color: #2d3436;
}

.loading {
  text-align: center;
  padding: 40px;
  color: var(--text-secondary);
}

.error {
  background: #fee;
  color: #c00;
  padding: 12px;
  border-radius: 6px;
  margin-bottom: 20px;
}

.search-box {
  width: 100%;
  padding: 12px;
  border: 1px solid var(--border-light);
  border-radius: 6px;
  font-size: 14px;
  margin-bottom: 16px;
  background: var(--bg-secondary);
  color: var(--text-primary);
}

.search-box:focus {
  outline: none;
  border-color: var(--accent);
}

.filter-group {
  display: flex;
  gap: 12px;
  margin-bottom: 16px;
  flex-wrap: wrap;
}

.filter-group select {
  padding: 8px 12px;
  border: 1px solid var(--border-light);
  border-radius: 6px;
  font-size: 14px;
  background: var(--bg-secondary);
  color: var(--text-primary);
}

.threshold-input {
  display: flex;
  align-items: center;
  gap: 8px;
}

.threshold-input input {
  width: 100px;
  padding: 8px 12px;
  border: 1px solid var(--border-light);
  border-radius: 6px;
  font-size: 14px;
  background: var(--bg-secondary);
  color: var(--text-primary);
}

.samples {
  color: var(--text-secondary);
  font-size: 12px;
  margin-top: 4px;
}

.detail-link {
  color: var(--accent);
  text-decoration: none;
  cursor: pointer;
}

.detail-link:hover {
  text-decoration: underline;
  color: var(--accent-hover);
}

.back-button {
  padding: 8px 16px;
  border: 1px solid var(--border-light);
  background: var(--bg-secondary);
  border-radius: 6px;
  cursor: pointer;
  font-size: 14px;
  margin-bottom: 16px;
  color: var(--text-primary);
}

.back-button:hover {
  background: var(--bg-tertiary);
}

.key-list {
  display: flex;
  flex-wrap: wrap;
  gap: 8px;
  margin-top: 8px;
}

.key-badge {
  padding: 4px 8px;
  background: var(--bg-tertiary);
  color: var(--text-primary);
  border: 1px solid var(--border-color);
  border-radius: 4px;
  font-size: 12px;
  font-weight: 500;
}

.service-list {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
  gap: 16px;
}

.service-card {
  padding: 16px;
  border: 1px solid var(--border-color);
  border-radius: 6px;
  cursor: pointer;
  transition: all 0.2s;
  background: var(--bg-secondary);
}

.service-card:hover {
  border-color: var(--accent);
  background: var(--bg-tertiary);
}

.service-name {
  font-weight: 600;
  margin-bottom: 8px;
}

.service-stats {
  font-size: 13px;
  color: var(--text-secondary);
}

/* Template styles for dark mode support */
.template-container {
  background: var(--bg-tertiary);
  border: 1px solid var(--border-color);
  border-radius: 8px;
  padding: 16px;
  margin-bottom: 20px;
}

.template-card {
  background: var(--bg-secondary);
  border: 1px solid var(--border-color);
  border-radius: 6px;
  padding: 12px;
  margin-bottom: 12px;
}

.template-card:last-child {
  margin-bottom: 0;
}

.template-label {
  font-size: 13px;
  color: var(--text-secondary);
  margin-bottom: 4px;
}

.template-code {
  font-size: 14px;
  color: var(--text-primary);
  word-break: break-word;
  font-family: 'Courier New', Courier, monospace;
}

.template-count {
  font-size: 16px;
  font-weight: bold;
  color: var(--accent);
}

/* Histogram bucket display */
.histogram-info {
  background: var(--bg-tertiary);
  padding: 12px;
  border-radius: 6px;
  margin-top: 8px;
}

.histogram-info p {
  margin: 6px 0;
  font-size: 14px;
}

.histogram-info strong {
  color: var(--text-primary);
  margin-right: 8px;
}

.template-percentage {
  font-size: 12px;
  color: var(--text-secondary);
}

.template-progress-bg {
  height: 6px;
  background: var(--border-color);
  border-radius: 3px;
  overflow: hidden;
  margin-bottom: 8px;
}

.template-progress-bar {
  height: 100%;
  background: linear-gradient(90deg, var(--accent), #42a5f5);
  transition: width 0.3s;
}

body.dark-mode .template-progress-bar {
  background: linear-gradient(90deg, var(--accent), #6bb0ff);
}

.template-example {
  font-size: 12px;
  color: var(--text-secondary);
  font-style: italic;
  padding: 8px;
  background: var(--bg-tertiary);
  border-radius: 4px;
  border-left: 3px solid var(--accent);
}

.template-count-text {
  font-size: 14px;
  color: var(--text-secondary);
}

.template-count-text-small {
  font-size: 0.85em;
}

/* Noisy Neighbors Styles */
.noisy-neighbors-header {
  margin-bottom: 20px;
}

.noisy-neighbors-header .subtitle {
  color: var(--text-secondary);
  margin-top: 8px;
  margin-bottom: 16px;
}

.threshold-control {
  display: flex;
  align-items: center;
  gap: 12px;
  margin-top: 16px;
  padding-top: 16px;
  border-top: 1px solid var(--border-color);
}

.threshold-control label {
  font-weight: 600;
  font-size: 14px;
}

.threshold-control input[type="number"] {
  width: 100px;
  padding: 8px 12px;
  border: 1px solid var(--border-color);
  border-radius: 4px;
  background: var(--bg-secondary);
  color: var(--text-primary);
  font-size: 14px;
}

.refresh-btn {
  padding: 8px 16px;
  background: var(--accent);
  color: white;
  border: none;
  border-radius: 4px;
  cursor: pointer;
  font-size: 14px;
  font-weight: 600;
  transition: background 0.2s;
}

.refresh-btn:hover {
  background: var(--accent-hover);
}

.signal-badge {
  display: inline-block;
  padding: 4px 8px;
  border-radius: 4px;
  font-size: 11px;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

.signal-metric {
  background: #e3f2fd;
  color: #1976d2;
}

.signal-span {
  background: #f3e5f5;
  color: #7b1fa2;
}

.signal-log {
  background: #fff3e0;
  color: #f57c00;
}

body.dark-mode .signal-metric {
  background: #1a237e;
  color: #90caf9;
}

body.dark-mode .signal-span {
  background: #4a148c;
  color: #ce93d8;
}

body.dark-mode .signal-log {
  background: #e65100;
  color: #ffcc80;
}

.cardinality-value {
  font-weight: 700;
  color: var(--accent);
}

.high-warning {
  background: rgba(255, 152, 0, 0.05);
}

body.dark-mode .high-warning {
  background: rgba(255, 152, 0, 0.1);
}

.item-list {
  display: flex;
  flex-direction: column;
  gap: 6px;
}

.item-entry {
  display: flex;
  align-items: center;
  gap: 8px;
  font-size: 13px;
}

.item-entry code {
  flex: 1;
  font-size: 12px;
}

.item-samples {
  color: var(--text-secondary);
  font-size: 11px;
}

.template-show-more {
  margin-top: 12px;
  text-align: center;
  font-size: 13px;
  color: var(--text-secondary);
}

/* Pattern Explorer Styles */
.pattern-explorer {
  padding: 20px;
}

.pattern-explorer .controls {
  display: flex;
  gap: 20px;
  padding: 20px;
  background: var(--bg-secondary);
  border-radius: 8px;
  margin-bottom: 20px;
  flex-wrap: wrap;
  align-items: flex-end;
}

.pattern-explorer .control-group {
  flex: 1;
  min-width: 200px;
}

.pattern-explorer .control-group label {
  display: flex;
  flex-direction: column;
  gap: 8px;
  font-size: 14px;
  font-weight: 500;
}

.pattern-explorer .control-group input[type="text"] {
  padding: 8px 12px;
  border: 1px solid var(--border-color);
  border-radius: 4px;
  background: var(--bg-primary);
  color: var(--text-primary);
  font-size: 14px;
}

.pattern-explorer .control-group input[type="number"] {
  padding: 8px 12px;
  border: 1px solid var(--border-color);
  border-radius: 4px;
  background: var(--bg-primary);
  color: var(--text-primary);
  font-size: 14px;
  width: 120px;
}

.pattern-explorer .refresh-button {
  padding: 8px 16px;
  background: var(--accent);
  color: white;
  border: none;
  border-radius: 4px;
  cursor: pointer;
  font-size: 14px;
  transition: background 0.2s;
}

.pattern-explorer .refresh-button:hover {
  background: var(--accent-hover);
}

.pattern-explorer .stats-summary {
  padding: 12px 20px;
  background: var(--bg-secondary);
  border-radius: 8px;
  margin-bottom: 20px;
  font-size: 14px;
  color: var(--text-secondary);
}

.pattern-explorer .patterns-list {
  display: flex;
  flex-direction: column;
  gap: 16px;
}

.pattern-card {
  background: var(--bg-secondary);
  border-radius: 8px;
  overflow: hidden;
  box-shadow: 0 1px 3px var(--shadow);
}

.pattern-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 16px 20px;
  cursor: pointer;
  transition: background 0.2s;
}

.pattern-header:hover {
  background: var(--bg-hover);
}

.pattern-info {
  display: flex;
  align-items: flex-start;
  gap: 12px;
  flex: 1;
}

.expand-icon {
  color: var(--text-secondary);
  font-size: 12px;
  margin-top: 2px;
  flex-shrink: 0;
}

.pattern-template {
  font-family: 'Courier New', monospace;
  font-size: 13px;
  line-height: 1.6;
  word-break: break-all;
}

.pattern-stats {
  display: flex;
  gap: 16px;
  align-items: center;
  flex-shrink: 0;
}

.total-count {
  font-weight: 600;
  color: var(--accent);
}

.service-count {
  color: var(--text-secondary);
  font-size: 14px;
}

.pattern-details {
  padding: 0 20px 20px 20px;
  border-top: 1px solid var(--border-color);
}

.example-log {
  padding: 16px 0;
  border-bottom: 1px solid var(--border-color);
}

.example-log h4 {
  font-size: 12px;
  font-weight: 600;
  margin-bottom: 8px;
  color: var(--text-secondary);
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

.example-log pre {
  background: var(--bg-primary);
  padding: 12px;
  border-radius: 4px;
  border: 1px solid var(--border-color);
  font-family: 'Courier New', monospace;
  font-size: 12px;
  overflow-x: auto;
  white-space: pre-wrap;
  word-wrap: break-word;
  color: var(--text-primary);
}

.severity-breakdown {
  display: flex;
  gap: 8px;
  padding: 16px 0;
  flex-wrap: wrap;
}

.severity-badge {
  padding: 4px 12px;
  border-radius: 12px;
  font-size: 12px;
  font-weight: 500;
  color: white;
}

.services-list {
  display: flex;
  flex-direction: column;
  gap: 12px;
}

.service-card {
  background: var(--bg-tertiary);
  border-radius: 6px;
  overflow: hidden;
  margin-bottom: 12px;
}

.service-header-info {
  display: flex;
  align-items: center;
  gap: 12px;
  padding: 12px 16px;
  background: var(--bg-hover);
}

.service-name {
  font-weight: 500;
  flex: 1;
}

.service-severities {
  display: flex;
  gap: 4px;
}

.severity-dot {
  width: 12px;
  height: 12px;
  border-radius: 50%;
  display: inline-block;
}

.service-details {
  padding: 16px;
  border-top: 1px solid var(--border-color);
}

.keys-section {
  margin-bottom: 20px;
}

.keys-section:last-child {
  margin-bottom: 0;
}

.keys-section h4 {
  font-size: 14px;
  font-weight: 600;
  margin-bottom: 12px;
  color: var(--text-secondary);
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

.keys-grid {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
  gap: 12px;
}

.key-item {
  background: var(--bg-secondary);
  padding: 12px;
  border-radius: 4px;
  border: 1px solid var(--border-color);
}

.key-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 8px;
}

.key-name {
  font-family: 'Courier New', monospace;
  font-size: 13px;
  font-weight: 500;
}

.cardinality {
  padding: 2px 8px;
  border-radius: 10px;
  font-size: 11px;
  font-weight: 600;
}

.cardinality.low {
  background: #10b981;
  color: white;
}

.cardinality.medium {
  background: #f59e0b;
  color: white;
}

.cardinality.high {
  background: #ef4444;
  color: white;
}

.sample-values {
  display: flex;
  flex-wrap: wrap;
  gap: 6px;
}

.sample-value {
  padding: 3px 8px;
  background: var(--bg-primary);
  border: 1px solid var(--border-color);
  border-radius: 3px;
  font-size: 11px;
  font-family: 'Courier New', monospace;
  color: var(--text-secondary);
}

.more-values {
  padding: 3px 8px;
  font-size: 11px;
  color: var(--text-secondary);
  font-style: italic;
}

/* CrossSignalCardinality Component */
.cross-signal-cardinality {
  padding: 20px;
}

.cross-signal-cardinality .header {
  margin-bottom: 24px;
}

.cross-signal-cardinality .header h2 {
  font-size: 24px;
  margin-bottom: 8px;
}

.cross-signal-cardinality .header p {
  color: var(--text-secondary);
  font-size: 14px;
}

.cross-signal-cardinality .controls {
  display: flex;
  gap: 20px;
  margin-bottom: 24px;
  flex-wrap: wrap;
}

.cross-signal-cardinality .control-group {
  display: flex;
  flex-direction: column;
  gap: 8px;
}

.cross-signal-cardinality .control-group label {
  font-size: 14px;
  font-weight: 500;
}

.cross-signal-cardinality .control-group input {
  padding: 8px 12px;
  border: 1px solid var(--border-color);
  border-radius: 4px;
  background: var(--bg-secondary);
  color: var(--text-primary);
  font-size: 14px;
  width: 150px;
}

.cross-signal-cardinality .keys-table {
  overflow-x: auto;
  margin-top: 20px;
}

.cross-signal-cardinality table {
  width: 100%;
  border-collapse: collapse;
  background: var(--bg-secondary);
  border-radius: 8px;
  overflow: hidden;
}

.cross-signal-cardinality th,
.cross-signal-cardinality td {
  padding: 12px;
  text-align: left;
  border-bottom: 1px solid var(--border-color);
}

.cross-signal-cardinality th {
  background: var(--bg-tertiary);
  font-weight: 600;
  font-size: 13px;
  text-transform: uppercase;
  color: var(--text-secondary);
}

.cross-signal-cardinality .signal-type-badge {
  display: inline-block;
  padding: 4px 10px;
  border-radius: 12px;
  font-size: 11px;
  font-weight: 600;
  color: white;
  text-transform: uppercase;
}

.cross-signal-cardinality .signal-name {
  font-family: 'Courier New', monospace;
  font-size: 13px;
}

.cross-signal-cardinality .key-scope {
  color: var(--text-secondary);
  font-size: 12px;
}

.cross-signal-cardinality .key-name code {
  background: var(--bg-tertiary);
  padding: 2px 6px;
  border-radius: 3px;
  font-size: 12px;
}

.cross-signal-cardinality .event-name {
  color: var(--text-secondary);
  font-size: 11px;
  margin-left: 4px;
}

.cross-signal-cardinality .sample-values {
  display: flex;
  gap: 6px;
  flex-wrap: wrap;
}

.cross-signal-cardinality .sample-value {
  background: var(--bg-tertiary);
  padding: 2px 6px;
  border-radius: 3px;
  font-size: 11px;
  font-family: 'Courier New', monospace;
  color: var(--text-secondary);
}

.cross-signal-cardinality .no-samples {
  color: var(--text-secondary);
  font-size: 12px;
  font-style: italic;
}

.cross-signal-cardinality .no-results {
  padding: 40px;
  text-align: center;
  color: var(--text-secondary);
  background: var(--bg-secondary);
  border-radius: 8px;
  margin-top: 20px;
}

/* MetadataComplexity Component */
.metadata-complexity {
  padding: 20px;
}

.metadata-complexity .header {
  margin-bottom: 24px;
}

.metadata-complexity .header h2 {
  font-size: 24px;
  margin-bottom: 8px;
}

.metadata-complexity .header p {
  color: var(--text-secondary);
  font-size: 14px;
}

.metadata-complexity .controls {
  display: flex;
  gap: 20px;
  margin-bottom: 24px;
  flex-wrap: wrap;
}

.metadata-complexity .control-group {
  display: flex;
  flex-direction: column;
  gap: 8px;
}

.metadata-complexity .control-group label {
  font-size: 14px;
  font-weight: 500;
}

.metadata-complexity .control-group input {
  padding: 8px 12px;
  border: 1px solid var(--border-color);
  border-radius: 4px;
  background: var(--bg-secondary);
  color: var(--text-primary);
  font-size: 14px;
  width: 150px;
}

.metadata-complexity .complexity-table {
  overflow-x: auto;
  margin-top: 20px;
}

.metadata-complexity table {
  width: 100%;
  border-collapse: collapse;
  background: var(--bg-secondary);
  border-radius: 8px;
  overflow: hidden;
}

.metadata-complexity th,
.metadata-complexity td {
  padding: 12px;
  text-align: left;
  border-bottom: 1px solid var(--border-color);
}

.metadata-complexity th {
  background: var(--bg-tertiary);
  font-weight: 600;
  font-size: 13px;
  text-transform: uppercase;
  color: var(--text-secondary);
}

.metadata-complexity th.sortable {
  cursor: pointer;
  user-select: none;
}

.metadata-complexity th.sortable:hover {
  background: var(--bg-hover);
}

.metadata-complexity .signal-type-badge {
  display: inline-block;
  padding: 4px 10px;
  border-radius: 12px;
  font-size: 11px;
  font-weight: 600;
  color: white;
  text-transform: uppercase;
}

.metadata-complexity .signal-name code {
  background: var(--bg-tertiary);
  padding: 2px 6px;
  border-radius: 3px;
  font-size: 12px;
}

.metadata-complexity .warning-badge {
  background: #ff9800;
  color: white;
  padding: 2px 8px;
  border-radius: 12px;
  font-size: 11px;
  font-weight: 600;
}

.metadata-complexity .ok-badge {
  color: var(--text-secondary);
  font-size: 12px;
}

.metadata-complexity .legend {
  margin-top: 32px;
  padding: 20px;
  background: var(--bg-secondary);
  border-radius: 8px;
  border: 1px solid var(--border-color);
}

.metadata-complexity .legend h3 {
  font-size: 16px;
  margin-bottom: 12px;
}

.metadata-complexity .legend ul {
  list-style: none;
  margin: 12px 0;
}

.metadata-complexity .legend li {
  margin: 8px 0;
  font-size: 14px;
}

.metadata-complexity .legend p {
  margin: 8px 0;
  font-size: 14px;
  color: var(--text-secondary);
}

.metadata-complexity .no-results {
  padding: 40px;
  text-align: center;
  color: var(--text-secondary);
  background: var(--bg-secondary);
  border-radius: 8px;
  margin-top: 20px;
}

/* Clickable table rows */
.clickable-row {
  transition: background-color 0.2s ease;
}

.clickable-row:hover {
  background-color: var(--bg-hover) !important;
}

.dark {
  --sidebar: hsl(240 5.9% 10%);
  --sidebar-foreground: hsl(240 4.8% 95.9%);
  --sidebar-primary: hsl(224.3 76.3% 48%);
  --sidebar-primary-foreground: hsl(0 0% 100%);
  --sidebar-accent: hsl(240 3.7% 15.9%);
  --sidebar-accent-foreground: hsl(240 4.8% 95.9%);
  --sidebar-border: hsl(240 3.7% 15.9%);
  --sidebar-ring: hsl(217.2 91.2% 59.8%);
}

@theme inline {
  --color-sidebar: var(--sidebar);
  --color-sidebar-foreground: var(--sidebar-foreground);
  --color-sidebar-primary: var(--sidebar-primary);
  --color-sidebar-primary-foreground: var(--sidebar-primary-foreground);
  --color-sidebar-accent: var(--sidebar-accent);
  --color-sidebar-accent-foreground: var(--sidebar-accent-foreground);
  --color-sidebar-border: var(--sidebar-border);
  --color-sidebar-ring: var(--sidebar-ring);
}
