@import './styles/theme.css';

@tailwind base;
@tailwind components;
@tailwind utilities;

/* Legacy CSS - keeping only essential overrides for non-migrated components */

:root {
  /* Legacy light mode colors - deprecated, use theme.css variables */
  --bg-primary: #f5f5f5;
  --bg-secondary: #ffffff;
  --bg-tertiary: #f8f8f8;
  --bg-hover: #f0f0f0;
  --text-primary: #333;
  --text-secondary: #666;
  --border-color: #e0e0e0;
  --border-light: #ddd;
  --shadow: rgba(0,0,0,0.1);
  --accent: #0066cc;
  --accent-hover: #0052a3;
  --sidebar: hsl(0 0% 98%);
  --sidebar-foreground: hsl(240 5.3% 26.1%);
  --sidebar-primary: hsl(240 5.9% 10%);
  --sidebar-primary-foreground: hsl(0 0% 98%);
  --sidebar-accent: hsl(240 4.8% 95.9%);
  --sidebar-accent-foreground: hsl(240 5.9% 10%);
  --sidebar-border: hsl(220 13% 91%);
  --sidebar-ring: hsl(217.2 91.2% 59.8%);
}

.dark,
body.dark-mode {
  /* Legacy dark mode colors - deprecated, use theme.css variables */
  --bg-primary: #1a1a1a;
  --bg-secondary: #2d2d2d;
  --bg-tertiary: #3a3a3a;
  --bg-hover: #404040;
  --text-primary: #e0e0e0;
  --text-secondary: #a0a0a0;
  --border-color: #404040;
  --border-light: #505050;
  --shadow: rgba(0,0,0,0.3);
  --accent: #4a9eff;
  --accent-hover: #6bb0ff;
}

